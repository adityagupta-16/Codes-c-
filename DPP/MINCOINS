#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int val=0;
	    
	    if(n%5==0)
	        {
	            val=n/10;
	            if(n%10==5)
	                val+=1;
	            cout<<val<<endl;
            }
        else
            cout<<-1<<endl;
	    }
	    
	
	return 0;
}
