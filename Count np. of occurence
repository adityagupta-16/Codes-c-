#include<bits/stdc++.h>
using namespace std;

int MAX=INT_MAX;
int n,arr[1000];

int main()
{
    cin>>n;
    int m;
    for (int i=1; i<=n; i++) 
		cin >> m, 							//6
	arr[m]++;								// 1 4 4 4 5 3	
    int count = 1;
    for (int i=2; i <= 5; i++)
        if (arr[i]>arr[count])
		 {
		 	count = i;
		 }
    cout << count<<endl;
    for(int i=1;i<=n;i++)
    {
    	if(arr[i]!=0)
    	{
    		cout<<i<<" : "<<arr[i]<<endl;
		}
	}
}
